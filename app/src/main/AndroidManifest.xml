<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.siddharth.fabkutadmin">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Admin.view.Login"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustResize|stateHidden">
        </activity>

        <activity
            android:name=".marketing.view.MarketingActivity"
            android:label="marketing"/>

        <activity
            android:name=".DataEntry.view.DataEntry"
            android:label="Data Entry"
            android:windowSoftInputMode="adjustResize|stateHidden"/>

        <activity
            android:name=".marketing.view.DataEntry_Market"
            android:label="data entry"
            android:windowSoftInputMode="adjustResize|stateHidden"/>

        <activity
            android:name=".marketing.view.Marketing_Tab"
            android:label="tab"
            android:windowSoftInputMode="adjustResize|stateHidden">
        </activity>

        <activity
            android:name=".Inventory.View.InventoryTab"
            android:label="inventory"/>


         </application>

</manifest>